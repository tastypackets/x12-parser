{
  "scripts": {
    "test": "mocha",
    "docs": "jsdoc -c jsdoc.json",
    "format": "prettier --write ./lib ./test --loglevel warn",
    "lint": "eslint ./lib --max-warnings 0 && prettier --check ./lib ./test --loglevel warn"
  },
  "name": "x12-parser",
  "version": "1.0.2",
  "description": "NodeJS X12 parser using streams",
