{
  "scripts": {
    "test": "vitest",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm run lint && npm run test:coverage && npm run build",
    "test:coverage": "vitest run --coverage ./test",
    "format": "prettier --write ./lib ./test --loglevel warn",
    "lint": "eslint ./lib/* --max-warnings 0",
    "check:ci": "npm run lint && npm run check:types && npm run check:formatting && npm run check:spelling && npm run test:coverage",
    "check:types": "tsc --noEmit",
    "check:formatting": "prettier --check ./lib ./test --loglevel warn",
    "check:spelling": "cspell ."
  },
  "name": "x12-parser",
  "version": "2.0.0",
  "description": "NodeJS X12 parser using streams",
  "main": "dist/lib/index.js",
  "repository": "https://github.com/tastypackets/x12-parser",
  "author": "Ezekiel Keator <20361796+tastypackets@users.noreply.github.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/lib",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-c8": "^0.30.1",
    "cspell": "^8.14.2",
    "eslint": "^8.39.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "bugs": {
    "url": "https://github.com/tastypackets/x12-parser/issues"
  },
  "homepage": "https://github.com/tastypackets/x12-parser",
  "keywords": [
    "X12",
    "ANSI",
    "EDI",
    "parser",
    "streams",
    "835",
    "837"
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true
  },
  "volta": {
    "node": "20.17.0"
  }
}
